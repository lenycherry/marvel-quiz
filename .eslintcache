i userSession est null, on affiche un loader le temps que le listener fasse sa vérif et redirige au besoin.\r\n    //si le listener détecte un user on affiche le contenu.\r\n    return userSession === null ? (\r\n        <Fragment>\r\n            <div className='loader'></div>\r\n            <p>Loading ...</p>\r\n        </Fragment>\r\n    ) : (\r\n            <div className='quiz-bg'>\r\n                <div className='container'>\r\n                    <Logout />\r\n                    <Quiz userData={userData} />\r\n                </div>\r\n\r\n            </div>\r\n        )\r\n}\r\n\r\nexport default Welcome\r\n","C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Login\\index.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Signup\\index.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\ErrorPage\\index.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Firebase\\index.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Firebase\\firebase.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Firebase\\context.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Logout\\index.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Quiz\\index.js",["79"],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\ForgetPassword\\index.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\Levels\\index.js",[],"C:\\Users\\Leny\\Desktop\\DEV\\Projets persos\\marvel-quiz\\src\\components\\ProgressBar\\index.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":43,"column":8,"nodeType":"86","endLine":43,"endColumn":21,"suggestions":"87"},{"ruleId":"88","severity":1,"message":"89","line":12,"column